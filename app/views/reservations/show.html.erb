<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @reservation.name %>
</p>


<p>
  <b>Address:</b>
  <%= @reservation.address %>
</p>

<p>
  <b>Contact:</b>
  <%= @reservation.contact %>
</p>

<p>
  <b>Date:</b>
  <%= @reservation.date %>
</p>

<p>
  <b>Timestart:</b>
  <%= @reservation.timeStart.strftime('%H:%M:%S:%p') %>
</p>

<p>
  <b>Timeend:</b>
  <%= @reservation.timeEnd.strftime('%H:%M:%S:%p') %>
</p>

<p>
  <b>Numguest:</b>
  <%= @reservation.numGuest %>
</p>

<p>
	<b>Total:</b>
	<%= @reservation.total_price%>
	
</p>

<%= display_package @reservation%>


<%= display_function_room @reservation%>



<%= link_to 'Edit', edit_reservation_path(@reservation) %> |
<%= link_to 'Back', reservations_path %>
